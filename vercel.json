{
  "rewrites": [
    { "source": "/", "destination": "/calendar.html" },
    { "source": "/api/data", "destination": "/api/data.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
